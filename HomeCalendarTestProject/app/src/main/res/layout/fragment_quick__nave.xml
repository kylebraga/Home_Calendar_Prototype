<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Quick_Nave"
   >

    <!-- TODO: Update blank fragment layout -->

    <Button
        android:layout_width="55dp"
        android:layout_height="45dp"
        android:id="@+id/button"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="10dp"
        android:layout_marginBottom="0dp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"/>

    <Button
        android:layout_width="55dp"
        android:layout_height="45dp"
        android:text="6am"
        android:id="@+id/sixamButt"
        android:textSize="12sp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:onClick="goTo6am"
        android:layout_below="@+id/button"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button"
        android:layout_alignEnd="@+id/button" />

    <Button
        android:layout_width="55dp"
        android:layout_height="45dp"
        android:id="@+id/button3"
        android:textSize="10dp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:layout_below="@+id/sixamButt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="55dp"
        android:id="@+id/nineamButt"
        android:textSize="12sp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:text="9am"
        android:onClick="goTo9am"
        android:layout_height="45dp"
        android:layout_below="@+id/button3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="55dp"
        android:id="@+id/button5"
        android:textSize="12sp"
        android:layout_below="@+id/nineamButt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/nineamButt"
        android:layout_alignEnd="@+id/nineamButt"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:layout_height="45dp"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/twelvepmButt"
        android:textSize="12sp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:text="12pm"
        android:layout_below="@+id/button5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button5"
        android:layout_alignEnd="@+id/button5"
        android:onClick="goTo12pm"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/button9"
        android:textSize="10dp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:layout_below="@+id/twelvepmButt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/twelvepmButt"
        android:layout_alignEnd="@+id/twelvepmButt" />

    <Button
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/threepmButt"
        android:textSize="12sp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:text="3pm"
        android:layout_below="@+id/button9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button9"
        android:layout_alignEnd="@+id/button9"
        android:onClick="goTo3pm"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/button7"
        android:textSize="10dp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:layout_below="@+id/threepmButt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/threepmButt"
        android:layout_alignEnd="@+id/threepmButt" />

    <Button
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/sixpmButt"
        android:textSize="12sp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:text="6pm"
        android:layout_below="@+id/button7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button7"
        android:layout_alignEnd="@+id/button7"
        android:onClick="goTo6pm"/>

    <Button
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/button2"
        android:textSize="10dp"
        android:background= "@color/ColorPrimary"
        android:textColor="@color/ColorThird"
        android:layout_below="@+id/sixpmButt"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/sixpmButt"
        android:layout_alignEnd="@+id/sixpmButt" />

</RelativeLayout>
